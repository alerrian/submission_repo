<h3>Items Returned: <%= @foods.count %></h3>

<% @foods[0..9].each do |food| %>
  <ul class='food'>
    <p class='upc'><%= food[:gtinupc] %></p>
    <p class='description'><%= food[:description] %></p>
    <p class='brand'><%= food[:brandowner] %></p>
    <p class='ingredients'><%= food[:ingredients] %></p>
    <p>---------------------------------------------</p>
  </ul>
<% end %>
